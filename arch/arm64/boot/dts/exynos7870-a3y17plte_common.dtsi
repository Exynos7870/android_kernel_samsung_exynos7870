			status = "okay";
			compatible = "abov,a96t3x6";
			pinctrl-names = "on_irq", "off_irq";
			pinctrl-0 = <&cfg_irq>;
			pinctrl-1 = <&cfg_input>;
			reg = <0x20>;
			interrupt-parent = <&gpa0>;
			abov,sda-gpio = <&gpc8 0 0>;
			abov,scl-gpio = <&gpc8 1 0>;
			abov,irq-gpio = <&gpa1 4 0>;
			abov,tkey_en-gpio = <&gpd1 6 0>;
